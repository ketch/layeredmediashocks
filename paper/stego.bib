@article{Toulorge2011,
abstract = {We study time step restrictions due to linear stability constraints of Runge-Kutta Discontinuous Galerkin methods on triangular grids. The scalar advection equation is discretized in space by the Discontinuous Galerkin method with either the Lax-Friedrichs flux or the upwind flux, and integrated in time with various Runge-Kutta schemes designed for linear wave propagation problems or non-linear applications. Von-Neumann-like analyses are performed on structured periodic grids made up of congruent elements, to investigate the influence of element shape on the stability restrictions. We assess CFL conditions based on different element size measures, among which only the radius of the inscribed circle and the shortest height prove appropriate, although they are not totally independent of the triangle shape. We explain their general behaviour with respect to element quality, and report the corresponding Courant numbers with both types of flux and polynomial order p ranging from 1 to 10, for use as guidelines in practical simulations. We also compare the performance of the Lax-Friedrichs flux and the upwind flux, and we draw general conclusions about the relative computational efficiency of RK schemes. The application of CFL conditions to two examples involving respectively in unstructured and a hybrid grid confirms our results, although it shows that local stability criteria tend to yield too restrictive conditions.},
author = {Toulorge, T. and Desmet, W.},
doi = {10.1016/j.jcp.2011.02.040},
issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {cfl condition,discontinuous galerkin,linear stability,runge-kutta},
month = mar,
title = {{CFL Conditions for Runge-Kutta Discontinuous Galerkin Methods on Triangular Grids}},
url = {http://dx.doi.org/10.1016/j.jcp.2011.02.040},
year = {2011}
}
@article{GRADY1998,
abstract = {The scattering of wave energy during shock propagation through heterogenous media is examined as an alternative to visco-plasticity as the physics underlying the formation of structured steady shock waves in polycrystalline metals. A theory based on a quasi-harmonic representation of scattered acoustic energy in solids is pursued and used to develop continuum constitutive relations to describe nonlinear wave propagation in heterogeneous solids. Resulting constitutive models are compared with shock wave profile data for metals.},
author = {GRADY, D},
doi = {10.1016/S0022-5096(98)00015-5},
issn = {00225096},
journal = {Journal of the Mechanics and Physics of Solids},
keywords = {inhomogeneous material,periodic media,plate impact,shock waves,stress waves},
mendeley-tags = {periodic media,shock waves},
month = oct,
number = {10},
pages = {2017--2032},
title = {{Scattering as a mechanism for structured shock waves in metals}},
url = {http://dx.doi.org/10.1016/S0022-5096(98)00015-5},
volume = {46},
year = {1998}
}
@article{Zhuang2003,
abstract = {In heterogeneous media, scattering due to interfaces/microstructure between dissimilar materials could play an important role in shock wave dissipation and dispersion. In this work, the influence of interface scattering on finite-amplitude shock waves was experimentally investigated by impacting flyer plates onto periodically layered polycarbonate/6061 aluminum, polycarbonate/304 stainless steel and polycarbonate/glass composites. Experimental results (obtained using velocity interferometer and stress gage) show that these periodically layered composites can support steady structured shock waves. Due to interface scattering, the effective shock viscosity increases with the increase of interface impedance mismatch, and decreases with the increase of interface density (interface area per unit volume) and loading amplitude. For the composites studied here, the strain rate within the shock front is roughly proportional to the square of the shock stress. This indicates that layered composites have much larger shock viscosity due to the interface/microstructure scattering in comparison with the increase of shock strain rate by the fourth power of the shock stress for homogeneous metals. Experimental results also show that due to the scattering effects, shock propagation in the layered composites is dramatically slowed down and the shock speed in composites can be lower than that either of its components.},
author = {Zhuang, S},
doi = {10.1016/S0022-5096(02)00100-X},
issn = {00225096},
journal = {Journal of the Mechanics and Physics of Solids},
keywords = {periodic media,shock waves},
mendeley-tags = {periodic media,shock waves},
month = feb,
number = {2},
pages = {245--265},
title = {{An experimental investigation of shock wave propagation in periodically layered composites}},
url = {http://dx.doi.org/10.1016/S0022-5096(02)00100-X},
volume = {51},
year = {2003}
}
